<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.munozcristhian.pholapsc">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/imagotipofondo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/imagotipofondo"
        android:supportsRtl="true"
        android:theme="@style/Theme.PholapSC">
        <activity
            android:name=".FotosAlbumActivity"
            android:exported="false" />
        <activity
            android:name=".PerfilEditActivity"
            android:exported="false" />
        <activity
            android:name=".PerfilEdit"
            android:exported="false" />
        <activity
            android:name=".ui.main.PerfilEdit"
            android:exported="false" />
        <activity
            android:name=".SeleccionActivity"
            android:exported="true" />
        <activity
            android:name=".CategoryActivity"
            android:exported="true"
            android:label="@string/title_activity_main_category"
            android:theme="@style/Theme.PholapSC.NoActionBar" />
        <activity
            android:name=".Perfil_Activity"
            android:exported="false"
            android:parentActivityName=".ConfiguracionesActivity" />
        <activity
            android:name=".ConfiguracionesActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            tools:ignore="IntentFilterExportedReceiver">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SingInActivity"
            android:exported="true" />
        <activity
            android:name=".ListSessionActivity"
            android:exported="true" />
        <activity
            android:name=".InfoImageActivity"
            android:exported="true" />
        <activity
            android:name=".SessionActivity"
            android:exported="true" />
    </application>

</manifest>