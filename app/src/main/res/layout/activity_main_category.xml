    <android.support.design.widget.TabLayout
        android:id="@+id/tabLayout_principal"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        app:tabBackground="@color/white"
        app:tabIndicator="@android:color/holo_green_light"
        app:tabTextColor="#000000"
        tools:ignore="MissingConstraints">

        <android.support.design.widget.TabItem
            android:id="@+id/tab_text_fotos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tab_text_fotos" />

        <android.support.design.widget.TabItem
            android:id="@+id/tab_text_albunes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tab_text_albunes" />

        <android.support.design.widget.TabItem
            android:id="@+id/tab_text_sesiones"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tab_text_sesiones" />
    </android.support.design.widget.TabLayout>

    <android.support.design.widget.AppBarLayout
        android:id="@+id/appBarLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:theme="@style/Theme.PholapSC.AppBarOverlay"
